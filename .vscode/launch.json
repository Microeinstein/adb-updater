{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "App updater",
            "type": "debugpy",
            "request": "launch",
            "module": "adb_updater",
            "cwd": "${workspaceFolder}",
            "autoReload": {
                "enable": true
            },
            "env": {
                "PYTHONPATH": "./src"
            },
            "justMyCode": false
        },
        {
            "name": "Snippet",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/trash/snippet.py",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}